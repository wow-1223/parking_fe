.container {
  padding: 10px;
}

.favorite-item {
  display: flex;
  margin-bottom: 10px;
  padding: 10px;
  border: 1px solid #ddd;
  border-radius: 8px;
}

.thumbnail {
  width: 80px;
  height: 80px;
  margin-right: 10px;
}

.info {
  display: flex;
  flex-direction: column;
  justify-content: center;
} 